{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Posts"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "id_post": {
      "type": "uid",
      "required": true
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "posts"
    },
    "Tipo": {
      "type": "enumeration",
      "enum": [
        "Nota",
        "Progresso"
      ],
      "required": true
    },
    "Nota": {
      "type": "integer",
      "max": 5
    },
    "Conteudo": {
      "type": "text",
      "maxLength": 1000
    },
    "comentarios": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comentario.comentario",
      "mappedBy": "post"
    }
  }
}
